[GENERAL]
startFrom = 0
stopAt = numSteps
numSteps = 100

dt = 0.1
variableDt = no
timeStepper = bdf3
targetCFL = 0.25

writeControl = runTime
writeInterval = 100.
writeDoublePrecision = no

dealiasing = yes
#filtering = explicit
#filterWeight = 0.01
#filterCutoffRatio = 0.84

[PRESSURE]
preconditioner = semg_xxt
residualTol = 1.0E-7
residualProj = yes
writeToFieldFile = yes

[VELOCITY]
residualTol = 1.0E-9
residualProj = no
density = 1.0
viscosity = -50.0
